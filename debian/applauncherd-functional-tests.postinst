#!/bin/sh
echo Creating symbolic links...

if [ -e /usr/bin/helloworld ]; then
	ln -s /usr/bin/helloworld /usr/bin/helloworld_1 
	ln -s /usr/bin/helloworld /usr/bin/helloworld_2 
fi
if [ -e /usr/bin/helloworld.launch ]; then
	ln -s /usr/bin/helloworld.launch /usr/bin/helloworld_1.launch 
	ln -s /usr/bin/helloworld.launch /usr/bin/helloworld_2.launch
fi
echo Done!
